      <TableView fx:id="tvResponsibleProject" layoutX="56.0" layoutY="116.0" prefHeight="200.0" prefWidth="489.0" />
      <FlowPane alignment="CENTER" columnHalignment="CENTER" hgap="90.0" layoutX="101.0" layoutY="358.0">
