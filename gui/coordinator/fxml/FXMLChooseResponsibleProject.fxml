<AnchorPane prefHeight="446.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.coordinator.controller.FXMLChooseResponsibleProjectController">
   <stylesheets>
      <URL value="@../../fxmlregister.css" />
   </stylesheets>
   <children>
      <FlowPane alignment="CENTER" columnHalignment="CENTER" hgap="90.0" prefHeight="82.0" prefWidth="600.0" style="-fx-background-color: #053d01;">
