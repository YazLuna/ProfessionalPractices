<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.practitioner.controller.FXMLListActivityController">
   <children>
      <Pane prefHeight="85.0" prefWidth="600.0" style="-fx-background-color: #053d01;">
         <children>
            <Button fx:id="btnLogOut" layoutX="475.0" layoutY="17.0" mnemonicParsing="false" onAction="#logOut" text="Cerrar sesion">
