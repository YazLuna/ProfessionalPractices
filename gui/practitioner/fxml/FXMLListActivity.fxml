         <font>
            <Font name="Arial Bold" size="15.0" />
         </font>
      </Button>
      <Button fx:id="btnCancel" layoutX="350.0" layoutY="321.0" mnemonicParsing="false" onAction="#cancel" style="-fx-background-color: #cc2d2d;" text="Cancelar" textFill="WHITE">
