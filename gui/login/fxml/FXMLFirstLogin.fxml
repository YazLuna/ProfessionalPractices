<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.layout.Pane?>
<?import java.net.URL?>

<AnchorPane id="AnchorPane" prefHeight="333.0" prefWidth="475.0" styleClass="mainFxmlClass"
            xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="gui.login.controller.FXMLFirstLoginController">
   <stylesheets>
      <URL value="@../../fxmlregister.css"/>
   </stylesheets>
   <Pane layoutY="-3.0" prefHeight="86.0" prefWidth="475.0" style="-fx-background-color: #053d01;">
      <Label layoutX="31.0" layoutY="16.0" prefHeight="54.0" prefWidth="419.0" text="Primer Inicio de Sesión"
             textFill="WHITE">
         <font>
            <Font name="Arial Bold" size="24.0"/>
         </font>
      </Label>
      <Button fx:id="btnLogOut" layoutX="389.0" layoutY="31.0" mnemonicParsing="false" onAction="#returnLogin"
              style="-fx-background-color: WHITE;" text="Regresar">
         <font>
            <Font name="Arial" size="12.0"/>
         </font>
      </Button>
   </Pane>
   <Button layoutX="279.0" layoutY="292.0" mnemonicParsing="false" onAction="#saveChanges"
           prefHeight="27.0" prefWidth="169.0" style="-fx-background-color: #053d01;" text="Guardar Cambios"
           textFill="WHITE">
      <font>
         <Font name="Arial Bold" size="15.0"/>
      </font>
   </Button>
   <Label fx:id="lbUser" layoutX="194.0" layoutY="108.0" text="Ingrese su nuevo nombre de Usuario">
      <font>
         <Font name="Arial" size="15.0"/>
      </font>
   </Label>
   <Label layoutX="194.0" layoutY="166.0" text="Ingrese su nueva Contraseña">
      <font>
         <Font name="Arial" size="15.0"/>
      </font>
   </Label>
   <TextField fx:id="tfUser" layoutX="194.0" layoutY="132.0" prefHeight="25.0" prefWidth="169.0"/>
   <ImageView fitHeight="120.0" fitWidth="135.0" layoutX="24.0" layoutY="94.0" pickOnBounds="true" preserveRatio="true">
      <Image url="@../../images/LogoSystem.png"/>
   </ImageView>
   <TextField fx:id="tfPassword" layoutX="194.0" layoutY="189.0" prefHeight="25.0" prefWidth="169.0"/>
   <TextField fx:id="tfPasswordConfirm" layoutX="194.0" layoutY="250.0" prefHeight="25.0" prefWidth="169.0"/>
   <Label layoutX="194.0" layoutY="227.0" text="Confirme su nueva Contraseña">
      <font>
         <Font name="Arial" size="15.0"/>
      </font>
   </Label>
</AnchorPane>
