<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TableColumn?>

<AnchorPane prefHeight="441.0" prefWidth="727.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="gui.administrator.controller.FXMLListCoordinatorController">
    <Button fx:id="btnCancel" layoutX="635.0" layoutY="391.0" mnemonicParsing="false" onAction="#cancel"
            style="-fx-background-color: #707070;" text="Regresar" textFill="WHITE">
        <font>
            <Font name="Arial Bold" size="12.0"/>
        </font>
    </Button>
    <Pane layoutX="-2.0" prefHeight="66.0" prefWidth="733.0" style="-fx-background-color: #053d01;">
        <Label layoutX="222.0" layoutY="16.0" text="Lista de Coordinadores" textFill="WHITE">
            <font>
                <Font name="System Bold" size="24.0"/>
            </font>
        </Label>
        <ImageView fitHeight="35.0" fitWidth="40.0" layoutX="588.0" layoutY="17.0" pickOnBounds="true"
                   preserveRatio="true">
            <Image url="@../../images/LogOut.png"/>
        </ImageView>
        <Button fx:id="btnLogOut" layoutX="620.0" layoutY="20.0" mnemonicParsing="false" onAction="#logOut"
                prefHeight="23.0" prefWidth="99.0" style="-fx-background-color: WHITE;" text="Cerrar Sesión">
            <font>
                <Font name="Arial" size="12.0"/>
            </font>
        </Button>
    </Pane>
    <TableView fx:id="tableCoordinators" layoutX="10.0" layoutY="88.0" prefHeight="292.0" prefWidth="712.0">
        <columns>
            <TableColumn fx:id="noPersonal" prefWidth="76.0" text="No. Personal"/>
            <TableColumn fx:id="name" prefWidth="110.0" text="Nombres"/>
            <TableColumn fx:id="lastName" prefWidth="127.0" text="Apellidos"/>
        <TableColumn fx:id="email" prefWidth="152.0" text="Correo"/>
        <TableColumn fx:id="alternateEmail" prefWidth="152.0" text="Correo Alterno"/>
        <TableColumn fx:id="phone" prefWidth="94.0" text="Teléfono"/>
        </columns>
    </TableView>
</AnchorPane>
