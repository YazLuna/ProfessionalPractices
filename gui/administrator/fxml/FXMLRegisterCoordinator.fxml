<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.control.RadioButton?>
<?import java.net.URL?>
<?import javafx.scene.layout.Pane?>

<AnchorPane id="AnchorPane" prefHeight="485.0" prefWidth="595.0" styleClass="mainFxmlClass"
            xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="gui.administrator.controller.FXMLRegisterCoordinatorController">
   <stylesheets>
      <URL value="@../../fxmlregister.css"/>
   </stylesheets>
   <Pane prefHeight="70.0" prefWidth="603.0" style="-fx-background-color: #053d01;">
      <Label layoutX="163.0" layoutY="23.0" text="Registrar Coordinador" textFill="WHITE">
         <font>
            <Font name="Arial Bold" size="24.0"/>
         </font>
      </Label>
      <Button fx:id="btnLogOut" layoutX="494.0" layoutY="13.0" mnemonicParsing="false" onAction="#logOut"
              style="-fx-background-color: WHITE;" text="Cerrar Sesión">
         <font>
            <Font name="Arial Bold" size="12.0"/>
         </font>
      </Button>
      <ImageView fitHeight="37.0" fitWidth="36.0" layoutX="467.0" layoutY="11.0" pickOnBounds="true"
                 preserveRatio="true">
         <Image url="@../../images/LogOut.png"/>
      </ImageView>
   </Pane>
   <Label layoutX="41.0" layoutY="105.0" text="No. Personal">
      <font>
         <Font name="Arial" size="14.0"/>
      </font>
   </Label>
   <Label layoutX="41.0" layoutY="135.0" text="Nombres">
      <font>
         <Font name="Arial" size="14.0"/>
      </font>
   </Label>
   <Label layoutX="41.0" layoutY="165.0" text="Apellidos">
      <font>
         <Font name="Arial" size="14.0"/>
      </font>
   </Label>
   <Label layoutX="41.0" layoutY="205.0" text="Género">
      <font>
         <Font name="Arial" size="14.0"/>
      </font>
   </Label>
   <Label layoutX="41.0" layoutY="244.0" text="Correo">
      <font>
         <Font name="Arial" size="14.0"/>
      </font>
   </Label>
   <Label layoutX="41.0" layoutY="275.0" text="Correo Alternativo">
      <font>
         <Font name="Arial" size="14.0"/>
      </font>
   </Label>
   <Label layoutX="41.0" layoutY="305.0" text="Teléfono">
      <font>
         <Font name="Arial" size="14.0"/>
      </font>
   </Label>
   <Button layoutX="428.0" layoutY="257.0" mnemonicParsing="false" onAction="#loadProfilePicture"
           style="-fx-background-color: #707070;" text="Subir Foto de Perfil" textFill="WHITE">
      <font>
         <Font name="Arial" size="14.0"/>
      </font>
   </Button>
   <ImageView fx:id="imgProfilePicture" fitHeight="129.0" fitWidth="143.0" layoutX="428.0" layoutY="114.0"
              pickOnBounds="true" preserveRatio="true">
   </ImageView>
   <TextField fx:id="tfStaffNumber" layoutX="167.0" layoutY="100.0" prefHeight="25.0" prefWidth="208.0"/>
   <TextField fx:id="tfName" layoutX="167.0" layoutY="130.0" prefHeight="25.0" prefWidth="208.0"/>
   <TextField fx:id="tfLastName" layoutX="167.0" layoutY="160.0" prefHeight="25.0" prefWidth="208.0"/>
   <TextField fx:id="tfEmail" layoutX="167.0" layoutY="239.0" prefHeight="25.0" prefWidth="208.0"/>
   <TextField fx:id="tfAlternateEmail" layoutX="167.0" layoutY="270.0" prefHeight="25.0" prefWidth="208.0"/>
   <TextField fx:id="tfPhone" layoutX="167.0" layoutY="300.0" prefHeight="25.0" prefWidth="208.0"/>
   <Button fx:id="btnRegister" layoutX="185.0" layoutY="430.0" mnemonicParsing="false" onAction="#register"
           style="-fx-background-color: #59ad55;" text="Aceptar" textFill="WHITE">
      <font>
         <Font name="Arial Bold" size="14.0"/>
      </font>
   </Button>
   <Button fx:id="btnCancel" layoutX="302.0" layoutY="430.0" mnemonicParsing="false" onAction="#cancel"
           prefHeight="27.0" prefWidth="81.0" style="-fx-background-color: #cc2d2d;" text="Cancelar" textFill="WHITE">
      <font>
         <Font name="Arial Bold" size="14.0"/>
      </font>
   </Button>
   <RadioButton fx:id="rbMale" layoutX="175.0" layoutY="204.0" mnemonicParsing="false" text="Masculino"/>
   <RadioButton fx:id="rbFemale" layoutX="288.0" layoutY="204.0" mnemonicParsing="false" text="Femenino"/>
   <Pane layoutX="30.0" layoutY="342.0" prefHeight="76.0" prefWidth="370.0" style="-fx-border-color: GREEN;">
      <Label layoutX="14.0" layoutY="46.0" text="Contraseña">
         <font>
            <Font name="Arial" size="14.0"/>
         </font>
      </Label>
      <Label layoutX="14.0" layoutY="12.0" text="Usuario">
         <font>
            <Font name="Arial" size="14.0"/>
         </font>
      </Label>
      <TextField fx:id="tfUserName" layoutX="138.0" layoutY="8.0" prefHeight="25.0" prefWidth="208.0"/>
      <TextField fx:id="tfPassword" layoutX="139.0" layoutY="41.0" prefHeight="25.0" prefWidth="208.0"/>
   </Pane>
</AnchorPane>
