<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.administrator.controller.FXMLDeleteCoordinatorController">
    <children>
        <ImageView fx:id="imgBackground" fitHeight="400.0" fitWidth="600.0" layoutY="1.0" pickOnBounds="true">
            <image>
                <Image url="@../../images/background.png" />
            </image>
        </ImageView>
        <ImageView fx:id="imgLogoAdministrator" fitHeight="70.0" fitWidth="68.0" layoutX="27.0" layoutY="12.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../../images/logoAdministrator.jpg" />
            </image>
        </ImageView>
        <Label fx:id="lbCoordinators" layoutX="232.0" layoutY="30.0" text="Coordinator" textFill="#1b4325">
            <font>
                <Font name="System Bold" size="24.0" />
            </font>
        </Label>
        <Button fx:id="btnCancel" layoutX="323.0" layoutY="328.0" mnemonicParsing="false" text="Cancel">
            <font>
                <Font name="Arial" size="12.0" />
            </font>
        </Button>
        <Button fx:id="btnLogOut" layoutX="510.0" layoutY="30.0" mnemonicParsing="false" text="Log Out">
            <font>
                <Font name="Arial" size="12.0" />
            </font>
        </Button>
      <Button fx:id="btnDelete" layoutX="232.0" layoutY="328.0" mnemonicParsing="false" text="Delete">
         <font>
            <Font name="Arial" size="12.0" />
         </font>
      </Button>
      <ImageView fx:id="imgLogOut" fitHeight="35.0" fitWidth="40.0" layoutX="470.0" layoutY="24.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../images/logout.png" />
         </image>
      </ImageView>
    </children>
</AnchorPane>
