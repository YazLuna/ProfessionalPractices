<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.AnchorPane?>
<?import java.net.URL?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.control.Label?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.administrator.controller.FXMLSectionTeacherController">
    <stylesheets>
        <URL value="@../css/fxmlsectionteacher.css" />
    </stylesheets>
    <children>
      <ImageView fitHeight="425.0" fitWidth="603.0" layoutX="-2.0" layoutY="1.0" pickOnBounds="true">
         <image>
            <Image url="@../../images/background.png" />
         </image>
      </ImageView>
        <Button fx:id="btnRegisterTeacher" layoutX="148.0" layoutY="94.0" mnemonicParsing="false"  prefHeight="59.0" prefWidth="221.0" text="Register Teacher">
            <font>
                <Font size="18.0" />
            </font>
        </Button>
        <Button fx:id="btnUpdateTeacher" layoutX="148.0" layoutY="162.0" mnemonicParsing="false"  prefHeight="59.0" prefWidth="221.0" text="Update Teacher">
            <font>
                <Font size="18.0" />
            </font>
        </Button>
        <Label fx:id="lbTeacherAdministration" layoutX="148.0" layoutY="52.0" text="Teacher Administration" textFill="#1c4d28">
            <font>
                <Font name="Arial Bold" size="24.0" />
            </font>
        </Label>
      <Button fx:id="btnDeleteTeacher" layoutX="148.0" layoutY="304.0" mnemonicParsing="false"  prefHeight="59.0" prefWidth="221.0" text="Delete  Teacher">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button fx:id="btnListTeacher" layoutX="148.0" layoutY="230.0" mnemonicParsing="false"  prefHeight="59.0" prefWidth="221.0" text="List Teacher">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <ImageView fx:id="imgHome" fitHeight="59.0" fitWidth="60.0" layoutX="22.0" layoutY="23.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../images/home.png" />
         </image>
      </ImageView>
      <Button fx:id="btnHome" blendMode="SOFT_LIGHT" layoutX="26.0" layoutY="28.0" mnemonicParsing="false"  prefHeight="49.0" prefWidth="52.0" />
      <Button fx:id="btnLogOut" layoutX="516.0" layoutY="28.0" mnemonicParsing="false"  text="Log Out" />
      <ImageView fx:id="imgLogOut" fitHeight="39.0" fitWidth="41.0" layoutX="475.0" layoutY="21.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../images/logout.png" />
         </image>
      </ImageView>
      <ImageView fx:id="imgRegister" fitHeight="58.0" fitWidth="66.0" layoutX="384.0" layoutY="95.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../images/register.png" />
         </image>
      </ImageView>
      <ImageView fx:id="imgUpdate" fitHeight="58.0" fitWidth="66.0" layoutX="384.0" layoutY="163.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../images/update.png" />
         </image>
      </ImageView>
      <ImageView fx:id="imgList" fitHeight="58.0" fitWidth="66.0" layoutX="384.0" layoutY="231.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../images/list.png" />
         </image>
      </ImageView>
      <ImageView fx:id="imgDelete" fitHeight="58.0" fitWidth="66.0" layoutX="384.0" layoutY="305.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../images/delete.png" />
         </image>
      </ImageView>
    </children>
</AnchorPane>
