            </ImageView>
            <ImageView fx:id="imgModifyTeacher" fitHeight="50.0" fitWidth="50.0" layoutX="153.0" layoutY="180.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../images/update.png" />
               </image>
            </ImageView>
         </children></Pane>
