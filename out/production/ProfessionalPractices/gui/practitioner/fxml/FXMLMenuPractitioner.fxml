            <Image url="@../../images/project.png" />
         </image>
      </ImageView>
      <ImageView fx:id="imgGenerate" fitHeight="45.0" fitWidth="50.0" layoutX="389.0" layoutY="149.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../images/register.png" />
         </image>
      </ImageView>
      <ImageView fx:id="imgUpload" fitHeight="45.0" fitWidth="45.0" layoutX="391.0" layoutY="203.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../images/update.png" />
         </image></ImageView>
      <ImageView fx:id="imgActivities" fitHeight="45.0" fitWidth="45.0" layoutX="392.0" layoutY="257.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../images/list.png" />
         </image></ImageView>
   </children>
</AnchorPane>
