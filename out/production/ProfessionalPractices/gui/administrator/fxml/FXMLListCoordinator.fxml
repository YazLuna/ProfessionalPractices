<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.administrator.controller.FXMLListCoordinatorController">
    <children>
        <ImageView fx:id="imgBackground" fitHeight="400.0" fitWidth="600.0" pickOnBounds="true">
            <image>
                <Image url="@../../images/background.png" />
            </image>
        </ImageView>
        <Label fx:id="lbCoordinators" layoutX="200.0" layoutY="30.0" text="Coordinators" textFill="#1b4325">
            <font>
                <Font name="System Bold" size="24.0" />
            </font>
        </Label>
        <Button fx:id="btnReturn" layoutX="514.0" layoutY="334.0" mnemonicParsing="false"  text="Return">
            <font>
                <Font name="Arial" size="12.0" />
            </font>
        </Button>
        <Button fx:id="btnLogOut" layoutX="510.0" layoutY="30.0" mnemonicParsing="false"  text="Log Out">
            <font>
                <Font name="Arial" size="12.0" />
            </font>
        </Button>
      <ImageView fx:id="imglogOut" fitHeight="35.0" fitWidth="35.0" layoutX="469.0" layoutY="24.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../images/logout.png" />
         </image>
      </ImageView>
    </children>
</AnchorPane>
