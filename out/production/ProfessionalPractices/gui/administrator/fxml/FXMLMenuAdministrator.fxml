            <Image url="@../../images/logoAdministrator.jpg" />
         </image>
      </ImageView>
      <Label fx:id="lbAdministrator" layoutX="35.0" layoutY="149.0" text="Administrator">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <Button fx:id="btnLogOut" layoutX="491.0" layoutY="21.0" mnemonicParsing="false"  prefHeight="27.0" prefWidth="79.0" text="LogOut">
         <font>
            <Font name="Arial Bold" size="12.0" />
         </font>
      </Button>
      <ImageView fx:id="imgLogOut" fitHeight="42.0" fitWidth="45.0" layoutX="449.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../images/logout.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
