<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.AnchorPane?>
<?import java.net.URL?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.control.Label?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.administrator.controller.FXMLMenuAdministratorController">
    <stylesheets>
        <URL value="@../css/fxmlmenuAdministrator.css" />
    </stylesheets>
   <children>
      <ImageView fx:id="imgBackground" fitHeight="386.0" fitWidth="600.0" layoutY="14.0" pickOnBounds="true">
         <image>
            <Image url="@../../images/background.png" />
         </image>
      </ImageView>
      <Button fx:id="btnCoordinator" layoutX="186.0" layoutY="124.0" mnemonicParsing="false"  prefHeight="50.0" prefWidth="258.0" text="Coordinators">
         <font>
            <Font name="Arial" size="18.0" />
         </font>
      </Button>
      <Button fx:id="btnTeacher" layoutX="186.0" layoutY="200.0" mnemonicParsing="false"  prefHeight="50.0" prefWidth="258.0" text="Teachers">
         <font>
            <Font name="Arial" size="18.0" />
         </font>
      </Button>
      <Label fx:id="lbProfessionalPractices" layoutX="186.0" layoutY="58.0" text="Professional Practices" textFill="#14431a">
         <font>
            <Font name="Arial Bold" size="24.0" />
         </font>
      </Label>
      <ImageView fx:id="imgLogoAdministrator" fitHeight="160.0" fitWidth="122.0" layoutX="14.0" layoutY="21.0" pickOnBounds="true" preserveRatio="true" smooth="false">
